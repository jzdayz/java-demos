<xml>
    <sql id="sql1">
        select CASE
        WHEN ISNUMERIC(empcode) = 1 THEN CONVERT(VARCHAR(10), SUBSTRING('0000', 0, 5 - LEN(empcode))) + empcode
        ELSE empcode
        END AS empcode from interface_empinfo where flag_sm = '1'
    </sql>
